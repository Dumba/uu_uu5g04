{
  "name": "uu5g04_icons",
  "version": "1.0.0",
  "namespace": "UU5.Icons",
  "description": "Icons for UU5",
  "scripts": {
    "start": "npm run start-dev",
    "test": "jest",
    "prestart-dev": "node tools/check-install.js && node tools/build-lib.js --skip-webpack",
    "start-dev": "webpack-dev-server --hot --inline --compress --config tools/webpack.config.js",
    "prestart-prod": "cross-env NODE_ENV=production node tools/build-lib.js --skip-webpack",
    "start-prod": "cross-env NODE_ENV=production webpack-dev-server --compress --config tools/webpack.config.js",
    "dist": "cross-env NODE_ENV=production node tools/build-lib.js",
    "prettify": "prettier --write \"{src,test}/**/*.js\""
  },
  "vendor": "Unicorn a.s.",
  "license": "UAF",
  "template": "uu5-library-1.7.0",
  "engines": {
    "node": ">=6.9.2"
  },
  "main": "dist-node/index.js",
  "jsnext:main": "dist-esm/index.js",
  "module": "dist-esm/index.js",
  "files": [
    "dist*",
    "doc"
  ],
  "dependencies": {},
  "uuBuildSettings": {
    "dependencies": {}
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babylon": "6.18.0",
    "circular-dependency-plugin": "4.2.0",
    "copy-webpack-plugin": "4.0.1",
    "cors": "2.8.4",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "empty": "0.10.1",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "1.0.4",
    "eslint": "4.9.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "4.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "jest-resolve": "21.2.0",
    "klaw-sync": "3.0.2",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "prettier": "1.7.4",
    "postcss-loader": "2.0.8",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.7.1",
    "wrapper-webpack-plugin": "1.0.0",
    "write-file-webpack-plugin": "4.1.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tools/jest-setup.js"
    ],
    "resolver": "<rootDir>/tools/jest-resolver.js",
    "moduleNameMapper": {
      "^uu5g04-bricks$": "uu5g04/dist-node/bricks/bricks-build.js",
      "^uu5g04-forms$": "uu5g04/dist-node/forms/forms-build.js",
      "^uu_plus4u5g01-app$": "uu_plus4u5g01/dist-node/app/app-build.js",
      "^uu_plus4u5g01-bricks$": "uu_plus4u5g01/dist-node/bricks/bricks-build.js",
      "^uu_plus4u5g01-productcatalogue$": "uu_plus4u5g01/dist-node/productcatalogue/productcatalogue-build.js",
      "^uu_plus4u5g01-uubmldraw$": "uu_plus4u5g01/dist-node/uubmldraw/uubmldraw-build.js"
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "modules": false
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions"
                ]
              }
            }
          ],
          "react"
        ]
      }
    }
  }
}
